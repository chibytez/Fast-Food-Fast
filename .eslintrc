{
    "root": true,
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
  
    "rules": {
      "linebreak-style": ["error","unix"],
      "func-names": ["error","never"],
      "import/no-unresolved": [2, { "commonjs": true }],
      "import/extensions": 1,
      "max-len": [2, 85]
    }
  }
  